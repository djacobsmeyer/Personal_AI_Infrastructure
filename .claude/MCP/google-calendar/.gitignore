credentials.json

# OAuth Tokens (NEVER COMMIT)
tokens/*.json
tokens/token*.json

# Environment files
.env
.env.local
.env.*.local

# Backup files
*.backup
*.bak
*~

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*

# OS files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Docker volumes
docker/volumes/

# Temporary files
tmp/
temp/
*.tmp

# IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# Node modules (if any)
node_modules/

# Build outputs
dist/
build/
*.tsbuildinfo

# Cache
.cache/
*.cache

# Test output
coverage/
.nyc_output/

# Keep directory structure but ignore contents
!credentials/.gitkeep
!tokens/.gitkeep
